# Environment Configuration
NODE_ENV=development

# Database
MONGO_URI=mongodb://localhost:27017/website_aksesoris

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_change_this_in_production_12345678901234567890123456789012
